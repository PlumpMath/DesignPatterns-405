<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
      <sectionGroup name="carFactories" >
        <section name="carFactory" type="AbstractFactory.CarFactoryConfiguration, AbstractFactory"/>
      </sectionGroup>
    </configSections>
  <appSettings>
    <add key="CarFactory" value="FactoryPatterns.AudiFactory"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <carFactories>
    <carFactory>
      <factory name="BMWFactory" value="FactoryPatterns.BMWFactory"/>
    </carFactory>
  </carFactories>
</configuration>